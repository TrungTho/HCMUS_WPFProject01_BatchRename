<Window x:Class="Project01_BatchRename.InputFileNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project01_BatchRename"
        mc:Ignorable="d"
        Title="InputFileNameDialog" Height="300" Width="300">
    <Canvas>
        <Label Content="Please input filename to save:" Canvas.Left="69" Canvas.Top="68"/>
        <TextBox Width="166" Height="25" Name="textboxToInput" Canvas.Left="69" Canvas.Top="99"/>
        <Button Content="OK" IsDefault="True" Click="Button_Click" Width="80" Height="25" Canvas.Left="69" Canvas.Top="142"/>
        <Button Content="Cancel" IsCancel="False" Click="cancelButton" Width="80" Height="25" RenderTransformOrigin="4.735,8.02" Canvas.Left="155" Canvas.Top="142"/>
    </Canvas>
</Window>
